{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "telegram-userinfo-bot",
  "main": "src/index.ts",
  "compatibility_date": "2025-08-15",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "vars": {
    "BOT_TOKEN": "your_bot_token_here"
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  }
}
